{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "start frontend",
      "type": "npm",
      "script": "start:frontend",
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "isBackground": true,
      "problemMatcher": {
        "pattern": "$tsc",
        "background": {
          "activeOnStart": true,
          "beginsPattern": {
            "regexp": "(.*?)"
          },
          "endsPattern": {
            "regexp": "ready in"
          }
        }
      }
    }
  ]
}
